<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="org.saravanakumar.es6" version="1.0.0" versionCode="2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>ES6</name>
  <description>
        ECMAScript
    </description>
  <author email="saravanakumargn@gmail.com" href="http://saravanakumar.org/">
      Saravanakumar
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="SplashScreenDelay" value="3000"/>
  <preference name="FadeSplashScreenDuration" value="2000"/>
  <preference name="FadeSplashScreen" value="false"/>
  <preference name="ShowSplashScreenSpinner" value="false"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="splash"/>
  <feature name="StatusBar">
    <param name="ios-package" onload="true" value="CDVStatusBar"/>
  </feature>
  <plugin name="cordova-plugin-device" spec="~1.1.3"/>
  <plugin name="cordova-plugin-console" spec="~1.0.4"/>
  <plugin name="cordova-plugin-whitelist" spec="~1.3.0"/>
  <plugin name="cordova-plugin-splashscreen" spec="~4.0.0"/>
  <plugin name="cordova-plugin-statusbar" spec="~2.2.0"/>
  <plugin name="ionic-plugin-keyboard" spec="~2.2.1"/>
  <platform name="android">
    <icon src="res/android/icon/drawable-ldpi-icon.png" qualifier="ldpi"/>
    <icon src="res/android/icon/drawable-mdpi-icon.png" qualifier="mdpi"/>
    <icon src="res/android/icon/drawable-hdpi-icon.png" qualifier="hdpi"/>
    <icon src="res/android/icon/drawable-xhdpi-icon.png" qualifier="xhdpi"/>
    <icon src="res/android/icon/drawable-xxhdpi-icon.png" qualifier="xxhdpi"/>
    <icon src="res/android/icon/drawable-xxxhdpi-icon.png" qualifier="xxxhdpi"/>
    <splash src="res/android/splash/drawable-land-ldpi-screen.png" qualifier="land-ldpi"/>
    <splash src="res/android/splash/drawable-land-mdpi-screen.png" qualifier="land-mdpi"/>
    <splash src="res/android/splash/drawable-land-hdpi-screen.png" qualifier="land-hdpi"/>
    <splash src="res/android/splash/drawable-land-xhdpi-screen.png" qualifier="land-xhdpi"/>
    <splash src="res/android/splash/drawable-land-xxhdpi-screen.png" qualifier="land-xxhdpi"/>
    <splash src="res/android/splash/drawable-land-xxxhdpi-screen.png" qualifier="land-xxxhdpi"/>
    <splash src="res/android/splash/drawable-port-ldpi-screen.png" qualifier="port-ldpi"/>
    <splash src="res/android/splash/drawable-port-mdpi-screen.png" qualifier="port-mdpi"/>
    <splash src="res/android/splash/drawable-port-hdpi-screen.png" qualifier="port-hdpi"/>
    <splash src="res/android/splash/drawable-port-xhdpi-screen.png" qualifier="port-xhdpi"/>
    <splash src="res/android/splash/drawable-port-xxhdpi-screen.png" qualifier="port-xxhdpi"/>
    <splash src="res/android/splash/drawable-port-xxxhdpi-screen.png" qualifier="port-xxxhdpi"/>
  </platform>
  <icon src="resources\android\icon\drawable-xhdpi-icon.png"/>
</widget>